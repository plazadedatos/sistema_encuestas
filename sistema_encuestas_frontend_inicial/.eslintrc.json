{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn",
    "no-unused-vars": "warn",
    "@next/next/no-img-element": "off",
    "react/no-unescaped-entities": "off"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  }
}
